FILE SHORTCUT.IAH=%path_database%discovery/pfts/%lang%/shortcut.pft
FILE GXML=%path_database%gizmo/gXML.*

[INDEX_DEFINITION]

INDEX Tw=^1Index basico^2Index basico^3Basic Index^4Index simple^d*^xTW ^yDATABASE^uTW_^mTW_
INDEX RE=^1Recurso^2Recurso^3Resource^4Ressource^xRE ^yDATABASE^uRE_^mRE_

[APPLY_GIZMO]


[FORMAT_NAME]

FORMAT standard.pft=^1Padrão^2Padrao^3default^4Défaut
